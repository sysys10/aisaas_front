{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "react",
  "extends": "./base.json",
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowJs": true,
    "jsx": "preserve",
    "noEmit": true,
    "esModuleInterop": true,
    "removeComments": true
  }
}
